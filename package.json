{
  "name": "robojs",
  "version": "5.5.7",
  "license": "MIT",
  "repository": "https://github.com/marcog83/RoboJS",
  "author": "Marco Gobbi",
  "main": "dist/robojs.min.js",
  "jsnext:main": "robojs.js",
  "module": "robojs.js",
  "typings": "src/type-definitions/robojs.d.ts",
  "scripts": {
    "build:babel-it": "babel ./dist/robojs.js --out-file ./dist/robojs.js",
    "build:bundle": "rollup -c",
    "build:uglify": "uglifyjs --compress unsafe --mangle -o dist/robojs.min.js dist/robojs.js",
    "build": "npm run build:bundle && npm run build:babel-it && npm run build:uglify",
    "release:major": "shelljs-release major",
    "release:minor": "shelljs-release minor",
    "release:patch": "shelljs-release patch",
    "test": "nyc --reporter=lcov mocha --require babel-core/register ./test/**",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "nsp": "nsp check",
    "docs": "typedoc --includeDeclarations --out docs src/type-definitions"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-import-export": "^1.0.2",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "jsdom": "^11.6.2",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.0.4",
    "mochawesome": "^3.0.2",
    "nyc": "^11.6.0",
    "rollup": "^0.57.0",
    "rollup-plugin-node-resolve": "^3.2.0",
    "shelljs-release": "^0.2.0",
    "uglify-js": "^3.3.15"
  }
}
