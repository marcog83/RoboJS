{
  "name": "robojs",
  "version": "5.0.0",
  "license": "MIT",
  "main": "dist/robojs.es6.min.js",
  "scripts": {
    "bundle": "jspm bundle-sfx src/core/robojs dist/robojs.js --format umd"
    ,"uglify": "uglifyjs --compress cascade,join_vars,if_return,unused,loops,booleans,evaluate,comparisons,conditionals,drop_debugger,sequences,properties,dead_code --mangle -o dist/robojs.min.js dist/robojs.js"
,"build": "npm run bundle && npm run uglify"
  },
  "jspm": {
    "main": "src/core/robojs",
    "format": "es6",
    "dependencies": {
      "ramda": "npm:ramda@^0.21.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4",
      "latest": "npm:babel-core@^5.8.24",
      "latest-runtime": "npm:babel-runtime@^5.8.24",
      "traceur": "github:jmcriffey/bower-traceur@0.0.93",
      "traceur-runtime": "github:jmcriffey/bower-traceur-runtime@0.0.93"
    }
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-preset-import-export": "^1.0.2",
    "babel-preset-latest": "^6.24.0",
    "grunt": "^0.4.5",
    "grunt-contrib-uglify": "^0.9.1",
    "jspm": "^0.16.19"
  }
}
